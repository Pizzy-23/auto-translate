{
    "name": "auto-translate-monorepo",
    "version": "1.0.0",
    "description": "Orquestrador do projeto de tradução.",
    "private": true,
    "scripts": {
        "start": "npm run dev",
        "dev": "concurrently \"npm:start:backend\" \"npm:start:frontend\"",
        "start:backend": "cd backend && python app.py",
        "start:frontend": "cd electron-app && npm run start:dev",
        "build": "cd electron-app && npm run build",
        "install:all": "npm install && cd electron-app && npm install"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "author": "Fizzy"
}